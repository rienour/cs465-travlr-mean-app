<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Dive Sites - Bhaccasyoniztas Beach Resort Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
      {{> header }}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Travel</h1>
							<ul id="sites">
                {{#each trips}}
                  <li>
                    <a href="/travel"><img src="images/{{ image }}" alt="Img"></a>
                    <h2><a href="/travel">{{ name }}</a></h2>
                    {{#each descriptions}}
                      <p>{{this}}</p>
                    {{/each}}
                  </li>
                {{/each}}
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
    {{> footer}}
	</div>
</body>
</html>
